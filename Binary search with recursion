#include<bits/stdc++.h>
using namespace std;
int arr[100000005];
int Search;
int BinarSearch(int start, int End)
{
    int mid=(start+End)/2;
    if(arr[mid]==Search){return mid;}
    if(start==End ) return 0;
    if(Search<arr[mid]){End=mid-1;}
    else {start=mid+1;}-
    BinarSearch(start,End);
}
int main()
{
    int n; cin>>n;
    for(int i = 0; i < n; i++){cin>>arr[i];}
    sort(arr,arr+n);
    cin>>Search;
    int start=0,End=n-1;
    cout<<"Found at "<< BinarSearch(start,End)<<endl;
    return 0;
}
